{
	"name": "retro-react",
	"version": "1.3.7",
	"private": false,
	"description": "A React component library for building retro-style websites",
	"keywords": [
		"react",
		"react-component",
		"react-component-library",
		"react-library",
		"react-ui",
		"react-ui-library",
		"retro",
		"retro-react",
		"retro-style",
		"retro-ui",
		"retro-ui-library",
		"ui",
		"ui-library",
		"webcore"
	],
	"author": "Retro React",
	"repository": {
		"type": "git",
		"url": "https://github.com/retro-react/retro-react"
	},
	"license": "MIT",
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./assets/*": {
			"import": "./dist/esm/assets/*",
			"require": "./dist/cjs/assets/*"
		}
	},
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "NODE_ENV=production rollup -c",
		"build:storybook": "storybook build -o docs-build",
		"changeset": "changeset",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx",
		"prepare": "husky install",
		"release": "changeset publish",
		"storybook": "NODE_ENV=development storybook dev -p 6006 --quiet",
		"test": "NODE_ENV=test jest",
		"test:coverage": "NODE_ENV=test jest --coverage",
		"test:watch": "NODE_ENV=test jest --watch",
		"format": "prettier **/*.tsx --write",
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{md,mdx,yml,json}": [
			"prettier --write"
		]
	},
	"browserslist": [
		"> 1%",
		"not dead",
		"not edge <= 18",
		"not ie 11",
		"not op_mini all"
	],
	"dependencies": {
		"@babel/runtime": "^7.21.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/plugin-transform-runtime": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.21.4",
		"@changesets/cli": "^2.26.1",
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@emotion/babel-plugin": "^11.10.6",
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@mdx-js/react": "^2.3.0",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.1.0",
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@rollup/plugin-terser": "^0.4.1",
		"@rollup/plugin-url": "^8.0.1",
		"@storybook/addon-a11y": "^7.0.7",
		"@storybook/addon-actions": "^7.0.7",
		"@storybook/addon-essentials": "^7.0.7",
		"@storybook/addon-interactions": "^7.0.7",
		"@storybook/addon-links": "^7.0.7",
		"@storybook/addon-mdx-gfm": "^7.0.7",
		"@storybook/react": "^7.0.7",
		"@storybook/react-webpack5": "^7.0.7",
		"@storybook/testing-library": "^0.1.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/jest": "^29.5.1",
		"@types/node": "^18.16.1",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.1",
		"@types/styled-components": "^5.1.26",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"@zerollup/ts-transform-paths": "^1.7.18",
		"babel-loader": "^9.1.2",
		"eslint": "8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-storybook": "^0.6.11",
		"eslint-plugin-testing-library": "^5.10.3",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jest-transform-stub": "^2.0.0",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rollup": "^3.21.0",
		"rollup-plugin-copy": "^3.4.0",
		"rollup-plugin-css-only": "^4.3.0",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-typescript2": "^0.34.1",
		"storybook": "^7.0.7",
		"theme-ui": "^0.15.7",
		"ts-jest": "^29.1.0",
		"tslib": "^2.5.0",
		"ttypescript": "^1.5.15",
		"typescript": "^4.3.5",
		"typescript-transform-paths": "^3.4.6",
		"webpack": "^5.81.0",
		"webpack-cli": "^5.0.2"
	},
	"peerDependencies": {
		"@emotion/react": "^11.0.0",
		"@emotion/styled": "^11.0.0",
		"react": "^16.8.0 || ^17 || ^18",
		"react-dom": "^16.8.0 || ^17 || ^18"
	},
	"engines": {
		"node": ">=16"
	}
}
